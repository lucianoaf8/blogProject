<%- include('partials/header'); -%>

<h1>Home</h1>

<p><%= homeStartingContent %></p>

<% posts.forEach(post => { %>

<h2><%= post.postTitle %> </h2>

<p>
    <%= post.postText.substring(0, 100) + "..." %>
    <a href="/posts/<%= post.postTitle %> ">Read More</a>
</p>

<% }); %>

<%- include('partials/footer'); -%>